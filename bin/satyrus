
## Standard Library
import sys

def main(argv: list):
    """ :: Satyrus CLI ::
        =================
    """
    try:
        from satyrus.cli import CLI
    except ImportError:
        raise SystemError("Satyrus3 is not correctly installed. See README for installation details.")
    else:
        CLI.run(argv)

if __name__ == '__main__':
    main(sys.argv)